f=\"isError.status\" :isError=\"isError\"></Alert>\n  </div>\n</template>\n\n<script>\nimport axios from \"axios\";\nimport Alert from \"../components/alert\";\n// const url = 'http://localhost:3000'\nconst url = 'https://kanban-serve.herokuapp.com'\n\nexport default {\n  name: \"Edit-Profile\",\n  components: {\n    Alert\n  },\n  data(){\n    return {\n      user:{\n        name:'',\n        email:'',\n        password:''\n      },\n      isError:{\n        status:false,\n        msg:[]\n      }\n    }\n  },\n  methods: {\n    cancel(){\n      this.$emit('isProfile', false)\n    },\n    updateProfile(){\n      axios({\n        url:`${url}/user`,\n        method:'put',\n        headers:{\n          access_token:localStorage.access_token\n        },\n        data:{\n          name:this.user.name,\n          email:this.user.email,\n          password:this.user.password\n        }\n      })\n      .then(data =>{\n        this.$emit('isProfile',false)\n      })\n      .catch(err =>{\n        this.isError.status = true;\n        this.isError.msg = err.response.data.msg\n      })\n    }\n  }\n};\n</script>\n\n<style>\n</style>"},"lineCount":null}},"error":null,"hash":"41da401948afbde58c08339ee85d0bee","cacheData":{"env":{}}}